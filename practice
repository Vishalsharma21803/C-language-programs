#include <stdio.h>
void prime(){
    int n;
    printf("Enter number: ");
    scanf("%d",&n);
    int count=0;
    for(int i=2;i<n;i++){
        if(n%i==0){
            count=count+1;
        }
    }
    if(count==0){
        printf("%d  a prime number.",n);
    }
    else{
        printf("%d not  a prime number",n);
    }


}
void main(){
    prime();
}

